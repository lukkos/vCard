        <h1>Symfony users</h1>
        <p class="lead blue">Click to show details.</p>
        
        {% raw %}
            
            <div style="display:none">{{flash | json}}</div>
        
        <div ng-show="flash.message" class="alert alert-{{flash.status}}" ><span class="glyphicon glyphicon-{{flash.icon}}"></span>&nbsp; {{flash.message}}</div>
        
        <div class="list-group">
            
            <a href="" ng-click="open(UsersList.0.item.id)" class="list-group-item" ng-show="UsersList.0">{{UsersList.0.item.username}} - {{UsersList.0.item.email}}</a>
            <a href="" ng-click="open(UsersList.1.item.id)" class="list-group-item" ng-show="UsersList.1">{{UsersList.1.item.username}} - {{UsersList.1.item.email}}</a>
            <a href="" ng-click="open(UsersList.2.item.id)" class="list-group-item" ng-show="UsersList.2">{{UsersList.2.item.username}} - {{UsersList.2.item.email}}</a>
            <a href="" ng-click="open(UsersList.3.item.id)" class="list-group-item" ng-show="UsersList.3">{{UsersList.3.item.username}} - {{UsersList.3.item.email}}</a>
            <a href="" ng-click="open(UsersList.4.item.id)" class="list-group-item" ng-show="UsersList.4">{{UsersList.4.item.username}} - {{UsersList.4.item.email}}</a>
            
        </div>
        
        <ul class="pagination pagination-sm">
            <li><a href="" ng-click="Pagination(UsersList.0.params,'prev')">&laquo;</a></li>
            <li ng-repeat="n in range(1,UsersList.0.params.pageCount)"  ng-class="{active: n==UsersList.0.params.current}" ><a ng-click="Pagination(UsersList.0.params,n)" href="">{{n}} <span class="sr-only">(current)</span></a></li>
            <li><a ng-click="Pagination(UsersList.0.params,'next')" href="">&raquo;</a></li>
        </ul>
        
        {% endraw %}